<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>RequestServlet</servlet-name>
    <servlet-class>com.cdsxt.action.RequestServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TestRequest1</servlet-name>
    <servlet-class>com.cdsxt.action.TestRequest1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TestRequest2</servlet-name>
    <servlet-class>com.cdsxt.action.TestRequest2</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>RequestServlet</servlet-name>
    <url-pattern>/requestServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TestRequest1</servlet-name>
    <url-pattern>/testRequest1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TestRequest2</servlet-name>
    <url-pattern>/testRequest2</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
