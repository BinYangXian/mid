<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>CookieServlet</servlet-name>
    <servlet-class>com.cdsxt.action.CookieServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CookieServlet2</servlet-name>
    <servlet-class>com.cdsxt.action.CookieServlet2</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CookieServlet3</servlet-name>
    <servlet-class>com.cdsxt.action.CookieServlet3</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>CookieServlet</servlet-name>
    <url-pattern>/cookieServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CookieServlet2</servlet-name>
    <url-pattern>/cookieServlet2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CookieServlet3</servlet-name>
    <url-pattern>/cookieServlet3</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
